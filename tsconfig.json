{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@common/*": ["common/*"],
      "@components/*": ["components/*"],
      "@locales/*": ["locales/*"],
      "@mocks/*": ["__mocks__/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"],
      "@contexts/*": ["contexts/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@artifacts/*": ["artifacts/*"]
    },
    "noImplicitAny": false
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/about-us/index.tsx"],
  "exclude": ["node_modules"]
}
